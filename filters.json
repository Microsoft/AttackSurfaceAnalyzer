{
  "WINDOWS": {
    "Scan": {
      "Registry": {
        "Key": {
          "Exclude": [
          ]
        },
      },
      "File": {
        "Path": {
          "Exclude": [
            "^[A-Z]:\\\\pagefile.sys$",
            "^[A-Z]:\\\\hiberfil.sys$",
            "^[A-Z]:\\\\swapfile.sys$",
          ]
        }
      }
    }
  },
  "MACOS": {
    "Monitor": {
      "File": {
        "Path": {
          "Exclude": [
            "/private/var/db/uuidtext",
            "/dev"
          ]
        }
      }
    },
    "Scan": {
      "File": {
        "Path": {
          "Exclude": [
            "^/.Spotlight-V100"
          ]
        }
      }
    }
  },
  "LINUX": {
    "Scan": {
      "File": {
        "Path": {
          "Exclude": [
          ]
        }
      }
    }
  }
}