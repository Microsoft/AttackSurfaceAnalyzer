{
  "Windows": {
    "Scan": {
      "Registry": {
        "Key": {
          "Exclude": [
            "^HKEY_CLASSES_ROOT\\\\WOW6432Node\\\\Interface$",
            "^HKEY_LOCAL_MACHINE\\\\SYSTEM\\\\CurrentControlSet\\\\Control$",
            "^HKEY_LOCAL_MACHINE\\\\SYSTEM\\\\DriverDatabase\\\\DriverPackages$",
            "^HKEY_LOCAL_MACHINE\\\\SYSTEM\\\\CurrentControlSet\\\\Enum$",
            "^HKEY_LOCAL_MACHINE\\\\SYSTEM\\\\CurrentControlSet\\\\Services\\\\DeviceAssociationService\\\\State\\\\Store$",
            "^HKEY_LOCAL_MACHINE\\\\SYSTEM\\\\CurrentControlSet\\\\Services\\\\mpssvc\\\\Parameters$",
            "^HKEY_LOCAL_MACHINE\\\\SYSTEM\\\\ControlSet001\\\\Enum$",
            "^HKEY_LOCAL_MACHINE\\\\SYSTEM\\\\ControlSet001\\\\Control$",
            "^HKEY_LOCAL_MACHINE\\\\SYSTEM\\\\ControlSet001\\\\Services$"
          ]
        },
        "Hive": {
          "Exclude": [
            "ClassesRoot"
          ]
        }
      },
      "File": {
        "Path": {
          "Exclude": [
            "[A-Z]:\\\\pagefile.sys",
            "[A-Z]:\\\\hiberfil.sys",
            "[A-Z]:\\\\swapfile.sys",
            "[A-Z]:\\\\Windows\\\\CSC",
            "[A-Z]:\\\\Windows\\\\System32\\\\LogFiles\\\\WMI\\\\RTBackup",
            "[A-Z]:\\\\Windows\\\\ServiceProfiles\\\\LocalService\\\\AppData\\\\Local\\\\Microsoft\\\\NGC"

          ]
        }
      }
    }
  },
  "Macos": {
    "Monitor": {
      "File": {
        "Path": {
          "Exclude": [
            "/private/var/db/uuidtext",
            "/Users",
            "/dev"
          ]
        }
      }
    },
    "Scan": {
      "File": {
        "Path": {
          "Exclude": [
            "^/private/var/db/uuidtext",
            "^/private/var/folders/", 
            "^/private/var/db/ConfigurationProfiles/Store", 
            "^/private/var/db/dslocal/nodes",
            "^/dev/",
            "^/.Spotlight-V100",
            "^/Library/Application Support/com.apple.TCC",
            "^/Users/[^/^\\\\]*/Library/Caches/com.apple.homed",
            "^/Users/[^/^\\\\]*/Library/Caches/com.apple.Safari.SafeBrowsing",
            "^/Users/[^/^\\\\]*/Library/Caches/com.apple.Safari",
            "^/Users/[^/^\\\\]*/Library/Caches/CloudKit/com.apple.Safari",
            "^/Users/[^/^\\\\]*/Library/Caches/com.apple.HomeKit.configurations",
            "^/Users/[^/^\\\\]*/Library/Caches/com.apple.safaridavclient",
            "^/Users/[^/^\\\\]*/Library/Cookies",
            "^/Users/[^/^\\\\]*/Library/VoiceTrigger/SAT",
            "^/Users/[^/^\\\\]*/Library/Metadata/com.apple.IntelligentSuggestions",
            "^/Users/[^/^\\\\]*/Library/Metadata/CoreSpotlight",
            "^/Users/[^/^\\\\]*/Library/PersonalizationPortrait",
            "^/Users/[^/^\\\\]*/Library/Containers/com.apple.stocks",
            "^/Users/[^/^\\\\]*/Library/Containers/com.apple.news",
            "^/Users/[^/^\\\\]*/Library/Containers/com.apple.mail",
            "^/Users/[^/^\\\\]*/Library/Containers/com.apple.iChat",
            "^/Users/[^/^\\\\]*/Library/Containers/com.apple.Safari",
            "^/Users/[^/^\\\\]*/Library/Containers/com.apple.Home",
            "^/Users/[^/^\\\\]*/Library/Containers/com.apple.VoiceMemos",
            "^/Users/[^/^\\\\]*/Library/Suggestions",
            "^/Users/[^/^\\\\]*/Library/Safari",
            "^/Users/[^/^\\\\]*/Library/Mail",
            "^/Users/[^/^\\\\]*/Library/HomeKit",
            "^/Users/[^/^\\\\]*/Library/Messages",
            "^/Users/[^/^\\\\]*/Library/IdentityServices",
            "^/Users/[^/^\\\\]*/Library/Application Support/CallHistoryDB",
            "^/Users/[^/^\\\\]*/Library/Application Support/com.apple.TCC",
            "^/Users/[^/^\\\\]*/Library/Application Support/CallHistoryTransactions",
            "^/Users/[^/^\\\\]*/Library/Application Support/MobileSync",
          ]
        }
      }
    }
  },
  "Linux": {
    "Scan": {
      "File": {
        "Path": {
          "Exclude": [
            "^/proc/",
            "^/usr/share/", 
            "^/usr/src/", 
            "^/snap/", 
            "^/run/systemd/", 
            "^/run/udev/"
          ]
        }
      }
    }
  },
}